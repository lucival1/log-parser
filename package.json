{
  "name": "backend_test",
  "version": "1.0.0",
  "type": "module",
  "description": "Technical test for backend dev at beamy.io",
  "main": "index.js",
  "scripts": {
    "generate_logs": "node initLogFiles.js",
    "emit_logs": "node lib/httpEmitter.js",
    "example_compute": "node exampleCompute.js",
    "test-og": "echo \"Error: no test specified\" && exit 1",
    "test": "jest --detectOpenHandles",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prebuild:level_1": "rimraf level_1/dist",
    "prebuild:level_2": "rimraf level_2/dist",
    "build:level_1": "nest build -p level_1/tsconfig.json",
    "build:level_2": "nest build -p level_2/tsconfig.json",
    "start:level_1": "node level_1/dist/src/main",
    "start:level_2": "node level_2/dist/main"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.0",
    "lodash": "^4.17.20",
    "uuid": "^8.3.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "redis": "^4.0.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.7.0-dev.20220408"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": ["level_1/src", "level_2/src"],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
